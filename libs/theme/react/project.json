{
  "root": "libs/theme/react",
  "sourceRoot": "libs/theme/react/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/theme/react/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/theme/react"],
      "options": {
        "jestConfig": "libs/theme/react/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
