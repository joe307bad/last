  "scripts": {
    "dev": "npx remix dev",
    "build": "yarn build:css && npx remix build",
    "postinstall": "npx remix setup node",
    "start": "NODE_ENV=development npx concurrently \"yarn dev:css\" \"yarn dev\"",
    "build:css": "npx tailwindcss -o ./app/tailwind.css",
    "dev:css": "npx tailwindcss -o ./app/tailwind.css --watch"
  },
  "dependencies": {
    "@remix-run/react": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "@remix-run/serve": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
